#include <iostream>

using namespace std;



int main(){

  int t,x,y,A;

  cin>>t;

  for(int i=1;i<=t;i++)

  {

      cin>>x>>y>>A;

      if(A>=x&&A<y)

      {

          cout<<"YES"<<endl;

      }

      else{

      cout<<"NO"<<endl;

      }

  }

    

// 	your code goes here

	return 0;

}
