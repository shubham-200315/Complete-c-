 pass or fail code chef

#include <iostream>

using namespace std;



int main() {

    int t,n,x,p,y;

    cin>>t;

    for(int i=1;i<=t;i++)

    {

        cin>>n>>x>>p;

        y=x*3;

        if((y-(n-x))>=p)

        {

            cout<<"PASS"<<endl;

        }

        else{

            cout<<"FAIL"<<endl;

        }

    }

	

	return 0;

}

