#include <iostream>

using namespace std;



int main() {

    int t,a,b,c;

    cin>>t;

    for(int i=1;i<=t;i++){

        cin>>a>>b>>c;

        if(a>b&&a>c)

        {

            cout<<a<<endl;

        }

        else if(b>c){

            cout<<b<<endl;

        }

        else{

            cout<<c<<endl;

        }

    }


	return 0;

}

