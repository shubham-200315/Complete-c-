#include <iostream>

using namespace std;



int main() {

    int t;

    double x,y,z;

    cin>>t;

    for(int i=1;i<=t;i++)

    {

        cin >>x>>z;

        y= (x*107)/100;

        if(z<=y)

        {

            cout<<"YES"<<endl;

        }

        else{

            cout<<"NO"<<endl;

        }

    }

	// your code goes here

	return 0;

}


