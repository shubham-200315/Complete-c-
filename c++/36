#include <bits/stdc++.h>

using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--){
	    float hra,da,n;
	    cin>>n;
	    if (n < 1500){
	        hra = (0.1) * n;
	        da = (0.9) * n;
	    }
	    else{
	        hra = 500;
	        da = (0.98) * n;
	    }
	    cout<<fixed<<setprecision(2)<<n + hra+da<<endl;
	}
	return 0;
}
