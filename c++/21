#include <iostream>

using namespace std;



int main() {

    int t,x,y,d;

    cin>>t;

    for(int i=1;i<=t;i++)

    {

        cin>>x>>y>>d;

        if(abs(x-y)<=d)

        {

            cout<<"YES"<<endl;

        }

        else

        {

            cout<<"NO"<<endl;

        }

    }

	// your code goes here

	return 0;

}

