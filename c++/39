#include <iostream>
using namespace std;

int main() {
    int T;
    cin>>T;
    for(int i=0; i<T; i++)
    {
    string S,T;
        cin>>S>>T;
        int x =S.length();
        string M="";
        for (int i=0; i<x;i++)
        {
            if(S[i]==T[i])
              M+='G';
            if(S[i]!=T[i])
                M+='B';
        }
        cout<<M<<endl;
    }
}
